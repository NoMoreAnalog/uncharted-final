.top-bar .fake-navigation
  background-image url("fake.jpg")
  height 125px
  background-size height 140px
  background-color #00adc6
  background-repeat no-repeat

.top-bar .chart-types
  font-size .9em
  height 40px
  background #ebebeb
  color #727272
  display flex
  justify-content center

.top-bar div
  display flex
  align-items center

.top-bar .chart-type
  margin 0 10px 0 50px

.top-bar .chart-type a
  display flex
  align-items center
  text-decoration none
  color #727272

.top-bar .chart-type img
  align-self center
  height 25px
  width 25px
  padding-right 10px